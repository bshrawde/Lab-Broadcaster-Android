<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                xmlns:fab="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:background="?attr/background"
                tools:context="cs490.labbroadcaster.MainActivity">
    <fragment class="cs490.labbroadcaster.MainFragment"
              android:layout_width="0dp"
              android:layout_height="match_parent"
              android:id="@+id/main1"
              android:layout_weight="1"
              tools:layout="@layout/activity_main_fragment"
        />
<!--    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="@android:color/darker_gray"/>-->

    <fragment class="cs490.labbroadcaster.ViewLabFragment"
              android:layout_width="0dp"
              android:layout_height="match_parent"
              android:id="@+id/viewlab"
              android:layout_weight="1.5"
              tools:layout="@layout/activity_view_lab_fragment"
        />
</LinearLayout>